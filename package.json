{
  "name": "alan-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "preview": "vite preview",
    "format": "yarn prettier ./src/**/*.{ts,tsx} --write",
    "interface:locales": "yarn run interface:locales:ru && yarn run interface:locales:kz",
    "interface:locales:kz": "i18next-resources-for-ts interface -i ./src/shared/i18n/locales/kz -o ./src/shared/@types/resources.d.ts",
    "interface:locales:ru": "i18next-resources-for-ts interface -i ./src/shared/i18n/locales/ru -o ./src/shared/@types/resources.d.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.7",
    "@tanstack/react-query": "^5.66.7",
    "antd": "^5.25.1",
    "axios": "^1.7.9",
    "i18next": "^24.2.2",
    "immer": "^10.1.1",
    "jwt-decode": "^4.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.29.0",
    "react-toastify": "^11.0.3",
    "tailwindcss": "^4.0.7",
    "zod": "^3.24.2",
    "zustand": "^4.5.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dayjs": "^1.11.11",
    "eslint": "8.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.3.0",
    "i18next-resources-for-ts": "^1.5.0",
    "prettier": "^3.2.5",
    "react-i18next": "^14.1.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.0"
  }
}
